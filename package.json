{
  "name": "loggatron",
  "version": "1.2.0-beta.0",
  "description": "A tree-shakeable, configurable logger that intercepts console methods with separators, colors, and emojis",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rollup -c",
    "prepublishOnly": "npm run build",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,json,md}\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "prepare": "husky",
    "dev:link": "npm run build && npm link",
    "dev:pack": "npm run build && npm pack",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:major": "standard-version --release-as major",
    "release:beta": "standard-version --prerelease beta",
    "release:beta:minor": "standard-version --prerelease beta --release-as minor",
    "release:beta:major": "standard-version --prerelease beta --release-as major",
    "release:alpha": "standard-version --prerelease alpha",
    "release:rc": "standard-version --prerelease rc",
    "publish:beta": "npm run release:beta && npm publish --tag beta",
    "publish:alpha": "npm run release:alpha && npm publish --tag alpha",
    "publish:rc": "npm run release:rc && npm publish --tag rc"
  },
  "keywords": [
    "logger",
    "console",
    "log",
    "intercept",
    "debugging",
    "tree-shakeable"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Raycas96/loggatron.git"
  },
  "homepage": "https://github.com/Raycas96/loggatron#readme",
  "bugs": {
    "url": "https://github.com/Raycas96/loggatron/issues"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.0",
    "prettier": "^3.2.4",
    "rollup": "^4.9.6",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "sideEffects": false,
  "lint-staged": {
    "*.{ts,json,md}": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "tslib": "^2.8.1"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
